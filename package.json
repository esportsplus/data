{
    "author": "ICJR",
    "dependencies": {
        "@esportsplus/utilities": "^0.27.2"
    },
    "description": "Compile-time data validation utility",
    "devDependencies": {
        "@esportsplus/typescript": "^0.21.0",
        "@types/node": "^25.0.3",
        "vite": "^7.3.0",
        "vitest": "^4.0.16"
    },
    "exports": {
        ".": {
            "import": "./build/index.js",
            "types": "./build/index.d.ts"
        },
        "./compiler/tsc": {
            "import": "./build/compiler/plugins/tsc.js",
            "require": "./build/compiler/plugins/tsc.js",
            "types": "./build/compiler/plugins/tsc.d.ts"
        },
        "./compiler/vite": {
            "import": "./build/compiler/plugins/vite.js",
            "types": "./build/compiler/plugins/vite.d.ts"
        }
    },
    "files": [
        "build"
    ],
    "main": "build/index.js",
    "name": "@esportsplus/data",
    "peerDependencies": {
        "esbuild": ">=0.17.0",
        "typescript": ">=5.0.0",
        "vite": ">=4.0.0"
    },
    "peerDependenciesMeta": {
        "esbuild": {
            "optional": true
        },
        "vite": {
            "optional": true
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/esportsplus/validation"
    },
    "scripts": {
        "build": "tsc",
        "build:test": "pnpm build && pnpm test",
        "-": "-",
        "prepare": "pnpm build",
        "prepublishOnly": "pnpm build",
        "--": "--",
        "test": "vitest run"
    },
    "sideEffects": false,
    "type": "module",
    "types": "build/index.d.ts",
    "version": "0.1.3"
}
