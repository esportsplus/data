{
    "author": "ICJR",
    "dependencies": {
        "@esportsplus/utilities": "^0.27.2"
    },
    "description": "Compile-time data validation utility",
    "devDependencies": {
        "@esportsplus/typescript": "^0.25.0",
        "@types/node": "^25.0.3",
        "vite": "^7.3.1",
        "vitest": "^4.0.16"
    },
    "exports": {
        ".": {
            "types": "./build/index.d.ts",
            "default": "./build/index.js"
        },
        "./compiler/tsc": {
            "types": "./build/compiler/plugins/tsc.d.ts",
            "default": "./build/compiler/plugins/tsc.js"
        },
        "./compiler/vite": {
            "types": "./build/compiler/plugins/vite.d.ts",
            "default": "./build/compiler/plugins/vite.js"
        }
    },
    "files": [
        "build"
    ],
    "main": "build/index.js",
    "name": "@esportsplus/data",
    "peerDependencies": {
        "esbuild": ">=0.17.0",
        "typescript": ">=5.0.0",
        "vite": ">=4.0.0"
    },
    "peerDependenciesMeta": {
        "esbuild": {
            "optional": true
        },
        "vite": {
            "optional": true
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/esportsplus/validation"
    },
    "scripts": {
        "build": "tsc",
        "build:test": "pnpm build && pnpm test",
        "-": "-",
        "prepare": "pnpm build",
        "prepublishOnly": "pnpm build",
        "--": "--",
        "test": "vitest run"
    },
    "sideEffects": false,
    "type": "module",
    "types": "build/index.d.ts",
    "version": "0.3.1"
}
